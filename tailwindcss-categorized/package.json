{
  "name": "eslint-plugin-tailwindcss-categorized",
  "version": "0.1.0",
  "description": "Tailwind CSS 클래스들을 카테고리별로 정렬해주는 ESLint 플러그인입니다.",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "npm run build && tsx --test tests/**/*.test.ts",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --fix"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "tailwind",
    "tailwindcss",
    "class-order",
    "categorized",
    "cva",
    "cn"
  ],
  "author": "bloominggrace",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bloominggrace/eslint-plugins.git",
    "directory": "tailwindcss-categorized"
  },
  "homepage": "https://github.com/bloominggrace/eslint-plugins#readme",
  "bugs": {
    "url": "https://github.com/bloominggrace/eslint-plugins/issues"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/eslint": "^9.6.1",
    "@types/estree": "^1.0.6",
    "@types/node": "^22.10.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^17.2.0",
    "prettier": "^3.8.1",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.54.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
